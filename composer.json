{
    "name": "frasmage/laravel-mediable",
    "description": "A package for easily managing and assigning media files to models with Laravel",
    "keywords": ["media", "image", "uploader", "eloquent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sean Fraser",
            "email": "frasmage@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0",
        "illuminate/filesystem": "~5.0",
        "illuminate/database": "~5.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~4.0",
        "spatie/laravel-glide": "^2.3"
    },
    "suggest": {
        "spatie/laravel-glide": "Assist with modifying images on the fly"
    },
    "autoload": {
        "psr-4": {
            "Frasmage\\Mediable\\": "src/"
        },
        "classmap": ["tests/"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
